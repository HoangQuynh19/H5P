{
  "version": "0.11.0",
  "name": "h5p",
  "description": "A helping tool for developing HTML5 Packages.",
  "keywords": [
    "development",
    "developing",
    "tool",
    "help",
    "html5",
    "package",
    "quiz",
    "interactive"
  ],
  "scripts": {
    "test": "jasmine",
    "jshint": "jshint bin/*.js lib/*.js"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6
    }
  },
  "preferGlobal": true,
  "homepage": "http://h5p.org",
  "author": "Joubel AS",
  "repository": {
    "type": "git",
    "url": "http://github.com/h5p/cli"
  },
  "bugs": {
    "url": "http://github.com/h5p/cli/issues"
  },
  "directories": {
    "lib": "./lib",
    "bin": "./bin"
  },
  "main": "./lib/h5p.js",
  "bin": "./bin/h5p-cli.js",
  "engines": {
    "node": ">=v0.10"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "archiver": ">=0.8.1",
    "body-parser": ">=1.14.1",
    "cross-spawn": "^5.0.1",
    "express": ">=4.13.3",
    "gitignore-parser": "0.0.2",
    "httpdispatcher": ">=0.4.0",
    "node.extend": ">=1.1.5",
    "request": ">=2.65.0",
    "sanitize-html": "^2.3.0",
    "tar-fs": ">=1.8.1"
  },
  "license": "MIT",
  "devDependencies": {
    "jasmine": "^2.4.1",
    "jshint": "^2.12.0"
  }
}
